#include<bits/stdc++.h>
using namespace std;

int main()
{
    // #ifndef ONLINE_JUDGE
    // // for getting input from input.txt
    // freopen("input.txt", "r", stdin);
    // // for writing output to output.txt
    // freopen("out.txt", "w", stdout);
    // #endif
    
    int N;
    cin>>N;
    while(N--){
        int arr[10];
        for(int i=0;i<10;i++){
            cin>>arr[i];
        }
        sort(arr,arr+10);
        cout<<arr[1]<<"\n";
    }
        
    return 0;
}
