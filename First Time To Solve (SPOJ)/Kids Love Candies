#include<bits/stdc++.h>
using namespace std;

int main()
{
    // #ifndef ONLINE_JUDGE
    // // for getting input from input.txt
    // freopen("input.txt", "r", stdin);
    // // for writing output to output.txt
    // freopen("out.txt", "w", stdout);
    // #endif
    
    int N;
    cin>>N;
    while(N--){
        int a,b;
        cin>>a>>b;
        int arr[a];
        int sum=0;
        for(int i=0;i<a;i++){
            cin>>arr[i];
            sum=sum+(arr[i]/b);
        }
        cout<<sum<<"\n";
    }
    return 0;
}
