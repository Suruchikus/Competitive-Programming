#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    // #ifndef ONLINE_JUDGE
    // // for getting input from input.txt
    // freopen("input.txt", "r", stdin);
    // // for writing output to output.txt
    // freopen("out.txt", "w", stdout);
    // #endif
    int T;
    cin>>T;
    while(T--){
        int n,m;
        cin>>n>>m;
        int flag=0;
        int arr[2][2];
        for(int i=0;i<n;i++){
            for(int k=0;k<2;k++){
                for(int l=0;l<2;l++){
                    cin>>arr[k][l];
                }
            }
            if(arr[0][1]==arr[1][0]){
                flag=1;
            }
        }
 
        if(m%2!=0){
            cout<<"NO"<<"\n";
        }
        else{
            if(flag==1){
                cout<<"YES"<<"\n";
            }
            else{
                cout<<"NO"<<"\n";
            }
        }
    }
    return 0;
}
