#include<bits/stdc++.h>
using namespace std;

int main()
{
    // #ifndef ONLINE_JUDGE
    // // for getting input from input.txt
    // freopen("input.txt", "r", stdin);
    // // for writing output to output.txt
    // freopen("out.txt", "w", stdout);
    // #endif
    
    int n;
    cin>>n;
    set<int> mp;
    int p;
    cin>>p;
    for(int i=0;i<p;i++){
        int a;
        cin>>a;
        mp.insert(a);
    }
    int q;
    cin>>q;
    for(int i=0;i<q;i++){
        int a;
        cin>>a;
        mp.insert(a);
    }
    if(mp.size()>=n){
        cout<<"I become the guy."<<"\n";
    }
    else{
        cout<<"Oh, my keyboard!"<<"\n";
    }
    return 0;
}
