#include<bits/stdc++.h>
using namespace std;

int main()
{
    // #ifndef ONLINE_JUDGE
    // // for getting input from input.txt
    // freopen("input.txt", "r", stdin);
    // // for writing output to output.txt
    // freopen("out.txt", "w", stdout);
    // #endif
    int T;
    cin>>T;
    while(T--){
        int n;
        cin>>n;
        int ans=0;
        while(n>0){
            int temp=n%10;
            n=n/10;
            ans=(ans*10)+temp;
        }
        cout<<ans<<"\n";
    }
    return 0;
}
